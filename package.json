  "homepage": "https://csaa-insurance.aaa.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --configuration=ds",
    "start:local": "ionic serve --configuration=ds -- --proxy-config=proxy.local.conf.json",
    "start:prod": "ionic serve --configuration=production",
    "build": "ionic build",
    "build:prod": "ionic build --prod",
    "rollbar:upload": "node src/app/micro-apps/csaa-mobile/source-map-upload.js --prod",
    "rollbar:debug-upload": "node src/app/micro-apps/csaa-mobile/source-map-upload.js",
    "test": "ng test",
    "test:watch": "ng test --watch=true",
    "test:coverage": "ng test --watch=false --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "postinstall": "patch-package && jetify",
    "qa:android": "ionic cordova prepare android --source-map --confirm --no-interactive && bundle exec fastlane android qa",
    "prepare:qa:ios": "ionic cordova prepare ios --source-map --confirm --no-interactive",
    "enterprise:ios": "npm run prepare:qa:ios && bundle exec fastlane enterprise",
    "lint-staged": "lint-staged",
    "pretty-quick": "pretty-quick",
    "build:ionic-env": "node ./scripts/build-ionicenv.js",
    "cap:sync": "npx cap sync",
    "cap:sync:android": "npx cap sync android",
    "cap:sync:ios": "npx cap sync ios",
    "on:ios": "node ./scripts/build-runon.js ios",
    "on:android": "node ./scripts/build-runon.js android",
    "prebuild": "npm run build:ionic-env"
  },
